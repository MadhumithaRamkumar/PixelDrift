
@{
    ViewBag.Title = "Dashboard";
}

<h2>Dashboard</h2>




@using (Html.BeginForm("CreateUser", "Home"))
{<div class="form-group">
        <div class="col-md-offset col-md-10">
            <button type="submit" class="btn btn-default">Create User</button>
        </div>
    </div>
}
@using (Html.BeginForm("LogOut", "Home"))
{
    <div class="form-group">
        <div class="col-md-offset col-md-2">
            <button type="submit" class="btn btn-default">Log Out</button>
        </div>
    </div>
}

<div class="form-group">
    <div class="col-md-offset-10 col-md-2">
        <button type="submit" class="btn btn-default">Add Files</button>
    </div>
</div>

<div class="form-group">
    <div class="col-md-offset col-md-1">
        <button type="submit" class="btn btn-default">Remove Files</button>
    </div>
</div>


<div class="form-group">
    <div class="col-md-5">
        <button type="submit" class="btn btn-default">Share Files</button>
    </div>
</div>


