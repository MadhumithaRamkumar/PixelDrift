@model PixelDrift.Models.User_login

@{
    ViewBag.Title = "Dashboard";
}

@{
    var data = TempData["UserId"];
}

<h2>Welcome @data to Pixel Drift</h2>





<div class="row row-no-gutters">
    @using (Html.BeginForm("CreateUser", "Home"))
    {<div class="form-group">
            <div class="col-sm-4">
                <button type="submit" class="btn btn-default">Create User</button>
            </div>
        </div>
    }
</div>
<div class="row">
    @using (Html.BeginForm("LogOut", "Home"))
    {
        <div class="form-group">
            <div class="col-sm-4">
                <button type="submit" class="btn btn-default">Log Out</button>
            </div>
        </div>
    }

</div>

<div class="row">
    @using (Html.BeginForm("Upload", "Home"))
    {
        <div class="form-group">
            <div class="col-sm-4">
                <button type="submit" class="btn btn-default">Add Files</button>
            </div>
        </div>
    }
</div>
<br />

@using (Html.BeginForm("Delete", "Home"))
{
    <div class="row">
        <div class="form-group">
            <div class="col-sm-4">
                <button type="submit" class="btn btn-default">Remove Files</button>
            </div>
        </div>

    </div>
}

<br />


@using (Html.BeginForm("ShareName", "Home"))
{
    <div class="row">
        <div class="form-group">
            <div class="col-sm-4">
                <button type="submit" class="btn btn-default">Share Files</button>
            </div>
        </div>

    </div>
}




